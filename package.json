{
  "name": "user-portal",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:client": "remix vite:build",
    "build:server": "esbuild --platform=node --format=esm ./server.ts --outdir=./",
    "dev": "tsx ./server.ts",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' yarn dev",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "nswag": "run-p nswag:*",
    "nswag:message": "nswag run ./.swagger/message.nswag version /runtime:Net60",
    "nswag:user": "nswag run ./.swagger/user.nswag version /runtime:Net60",
    "nswag:notification": "nswag run ./.swagger/notification.nswag version /runtime:Net60"
  },
