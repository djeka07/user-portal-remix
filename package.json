    "i18next-browser-languagedetector": "8.0.0",
    "i18next-fs-backend": "2.3.2",
    "i18next-http-backend": "2.6.1",
    "isbot": "5.1.17",
    "polished": "4.3.1",
    "react": "19.0.0-rc-49496d49-20240814",
    "react-dom": "19.0.0-rc-49496d49-20240814",
    "react-hook-form": "7.53.0",
